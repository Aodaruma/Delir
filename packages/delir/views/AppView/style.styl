.preferenceEnter
    opacity 0
    transform scale(1.2)

.preferenceEnterActive
    opacity 1
    transform scale(1)

    transition-property opacity, transform
    transition-duration .4s

.preferenceLeave
    opacity 1
    transform scale(1)

.preferenceLeaveActive
    opacity 0
    transform scale(1.2)

    transition-property opacity, transform
    transition-duration .4s
