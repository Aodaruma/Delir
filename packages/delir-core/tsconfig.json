{
    "compilerOptions": {
        "baseUrl": "./src",
        "target": "es2015",
        "module": "es2015",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true
    }
}
