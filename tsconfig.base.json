{
    "compileOnSave": false,
    "compilerOptions": {
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "isolatedModules": true,
        "downlevelIteration": true,
        "allowJs": true,
        "target": "es2016",
        "module": "es2015",
        "moduleResolution": "node",
        "jsx": "react",
        "lib": [
            "dom",
            "dom.iterable",
            "es2015",
            "es2016",
        ]
    }
}
