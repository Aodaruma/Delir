{
    "name": "delir-plugin-audio",
    "version": "0.0.1",
    "author": "Ragg <ragg.devpr@gmail.com>",
    "main": "audio-layer.js",
    "engines": {
        "delir": "0.0.x"
    },
    "delir": {
        "feature": "CustomLayer",
        "targetApi": {
            "renderer": "0.0.x"
        },
        "acceptFileTypes": {
            "audio/wav": "source",
            "audio/webm": "source",
            "audio/mpeg": "source",
            "audio/ogg": "source"
        }
    }
}
