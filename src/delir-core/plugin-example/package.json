{
  "name": "delir-plugin-example",
  "version": "0.0.0",
  "description": "Example plugin for Delir",
  "main": "dist/index.js",
  "repository": "git@github.com:Ragg-/delir-plugin-example.git",
  "author": "Ragg <ragg.devpr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/webpack --watch",
    "build": "./node_modules/.bin/webpack"
  },
  "engines": {
    "delir": "0.0.x"
  },
  "delir": {
    "feature": "CustomLayer",
    "targetApi": {
        "renderer": "0.0.x"
    },
    "acceptFileTypes": {
        "video/mp4": "source"
    }
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.2",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2"
  }
}
